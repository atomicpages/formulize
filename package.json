{
  "name": "formulize",
  "version": "0.1.0",
  "description": "formula UI generator",
  "main": "dist/formulize.umd.js",
  "typings": "dist/types/formulize.d.ts",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "lint:fix": "pnpm lint --fix",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "build:sass": "sass style/formulize.scss style/formulize.css",
    "typedoc": "typedoc --out docs --target es6 --theme minimal --mode file src --exclude **/*.spec.ts",
    "pretest": "pnpm build:sass",
    "test": "vitest --run",
    "posttest": "rm style/*.css*"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KennethanCeyer/formulize.git"
  },
  "keywords": [
    "plugin",
    "formula",
    "equasion",
    "math",
    "generator",
    "builder",
    "wyswyg",
    "jquery",
    "pignose"
  ],
  "author": "PIGNOSE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KennethanCeyer/formulize/issues"
  },
  "homepage": "https://github.com/KennethanCeyer/formulize#readme",
  "dependencies": {
    "metric-parser": "^0.0.12"
  },
  "devDependencies": {
    "@djthoms/eslint-config": "^6.0.1",
    "@djthoms/prettier-config": "^4.3.0",
    "@types/jquery": "^3.5.16",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.5.1",
    "@vitest/coverage-v8": "^0.34.2",
    "esbuild": "^0.19.2",
    "husky": "^8.0.3",
    "jquery": "^3.7.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "lodash.camelcase": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.66.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vitest": "^0.34.2"
  }
}
